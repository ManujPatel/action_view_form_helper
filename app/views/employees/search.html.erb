<% if flash[:notice] %> 
  <% flash.each do |name,msg| %>
    <h3 style="color:green;"><%= msg %></h3>
  <% end %>
<% end %>
<%= form_with url: "/search", method: :get do |form| %>
  <%= form.label :query, "Search employee by employee id:" %>
  <%= form.text_field :query %>
  <%= form.submit "Search" %>
<% end %>